{
  "name": "recipeapp-api",
  "version": "1.0.0",
  "description": "RecipeApp Backend API",
  "main": "index.js",
  "scripts": {
    "deploy": "vercel --prod",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f mongodb",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "dev:local": "cp .env.local .env && vercel dev",
    "test:db": "node scripts/test-db-connection.js"
  },
  "dependencies": {
    "@google-cloud/vision": "^5.3.4",
    "@react-email/button": "^0.2.1",
    "@react-email/components": "^1.0.7",
    "@react-email/container": "^0.0.16",
    "@react-email/heading": "^0.0.16",
    "@react-email/html": "^0.0.12",
    "@react-email/section": "^0.0.17",
    "@react-email/text": "^0.1.6",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.3",
    "mongodb": "^6.3.0",
    "nanoid": "^3.3.11",
    "openai": "^4.26.0",
    "react-email": "^5.2.8",
    "resend": "^6.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.5",
    "@types/react": "^19.2.14",
    "@vercel/node": "^3.0.21",
    "typescript": "^5.3.3"
  }
